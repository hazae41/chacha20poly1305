{
  "type": "module",
  "name": "@hazae41/chacha20poly1305",
  "version": "1.0.2",
  "description": "ChaCha20Poly1305 adapter for WebAssembly and JS implementations",
  "homepage": "https://github.com/hazae41/chacha20poly1305",
